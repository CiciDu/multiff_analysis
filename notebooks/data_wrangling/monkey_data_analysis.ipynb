   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "XLAmip37uu9i"
   },
   "outputs": [],
   "source": [
    "plot_statistics.fit_and_plot_linear_regression(distance_array, num_stops_array, show_regression = True)\n",
    "plt.title('Numbers of Stops vs. Distance Traveled')\n",
    "plt.xlabel('Distance Traveled', labelpad=15)\n",
    "plt.ylabel('Numbers of Stops')\n",
    "plt.show()\n"
   ]
  },
  {
   "attachments": {},
